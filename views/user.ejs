<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Data</title>
    <style>
        body {
            background-color: #121212;
            color: #ffffff;
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
        }

        .container {
            max-width: 1000px;
            margin: 50px auto;
            padding: 20px;
            background-color: #1e1e1e;
            border-radius: 10px;
            box-shadow: 0 4px 15px rgba(127, 0, 255, 0.5);
        }

        h1 {
            text-align: center;
            color: #7f00ff;
            margin-bottom: 20px;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }

        table, th, td {
            border: 1px solid #7f00ff;
        }

        th, td {
            padding: 10px;
            text-align: left;
        }

        th {
            background-color: #7f00ff;
            color: #ffffff;
        }

        tr:nth-child(even) {
            background-color: #2a2a2a;
        }

        tr:hover {
            background-color: #3a3a3a;
        }

        .btn{
            padding: 10px 15px;
            background: #7f00ff;
            color: black;
            border: none;
            border-radius: 5px;
            outline: none;
            margin-top: 10px;
            margin-left: 10px;
        }
    </style>
</head>
<body>
    <a href="/admin-dashboard"><button class="btn">Back To Admin DashBoard</button></a>
    <div class="container">
        <h1>User Data</h1>
        <table>
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Username</th>
                    <th>Age</th>
                    <th>Email</th>
                    <th>Mobile</th>
                    <th>Address</th>
                    <th>Aadhar</th>
                    <th>Role</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.name %></td>
                        <td><%= user.username %></td>
                        <td><%= user.age %></td>
                        <td><%= user.email %></td>
                        <td><%= user.mobile %></td>
                        <td><%= user.address %></td>
                        <td><%= user.addhar %></td>
                        <td><%= user.role %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>
